{
	"name": "ai-prompt-helper",
	"version": "0.0.2",
	"description": "helper for ai prompt",
	"scripts": {
		"ng": "ng",
		"start": "node files_to_json.js && ng serve",
		"build": "node files_to_json.js && ng build",
		"watch": "node files_to_json.js && ng build --watch --configuration development",
		"test": "node files_to_json.js && ng test",
		"package": "node files_to_json.js && npm run build --aot --build-optimizer=true --progress=false",
		"prettier:fix": "prettier --config ./.prettierrc --list-different \"src/**/*{.ts,.js,.json,.scss,.html}\" --write"
	},
	"repository": {
		"type": "git",
		"url": "git+https://gitlab.com/sssunsha1/ai-prompt-helper.git"
	},
	"author": "",
	"license": "MIT",
	"bugs": {
		"url": "https://gitlab.com/sssunsha1/ai-prompt-helper/issues"
	},
	"homepage": "https://gitlab.com/sssunsha1/ai-prompt-helper#readme",
	"dependencies": {
		"@angular/animations": "17.0.1",
		"@angular/common": "17.0.1",
		"@angular/compiler": "17.0.1",
		"@angular/core": "17.0.1",
		"@angular/forms": "17.0.1",
		"@angular/platform-browser": "17.0.1",
		"@angular/platform-browser-dynamic": "17.0.1",
		"@angular/router": "17.0.1",
		"@fundamental-ngx/core": "0.48.4",
		"@fundamental-styles/common-css": "^0.30.2",
		"fundamental-styles": "0.33.2",
		"lodash-es": "^4.17.21",
		"marked": "^9.0.0",
		"ngx-markdown": "^17.0.0",
		"prismjs": "^1.29.0",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"zone.js": "0.14."
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "17.0.1",
		"@angular/cli": "17.0.1",
		"@angular/compiler-cli": "17.0.1",
		"@types/jasmine": "~4.3.0",
		"jasmine-core": "~4.6.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
